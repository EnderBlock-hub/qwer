		<script>
			function ValidatorTextComment(){
				var input = document.getElementById('comment')
				var butt = document.getElementById('btn-comment')
				input.oninput = () => {
					var inputSplit = input.value.split('')
					var massEmpty = []
					for (var i of inputSplit){
						if (i === '' || i === ' ') {
							massEmpty.push(' ')
						}
					}
					if (inputSplit.length !== massEmpty.length){
						butt.addEventListener('click', (event) => {
							event.defaultPrevented = false
						})
					} else {
						butt.addEventListener('click', (event) => {
							event.preventDefault()
						})
					}
				}
			}
		</script>
